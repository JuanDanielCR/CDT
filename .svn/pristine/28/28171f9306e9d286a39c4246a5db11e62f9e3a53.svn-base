<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<!-- Head -->
<head>
<jsp:text>
	<![CDATA[
	<script
		src="${pageContext.request.contextPath}/pages/entrevistas/gestionar-lista-aspirantes/js/index.js"
		type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<!-- Body -->
<body>
	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.8.3-4_TITLE"></s:text>
			</h1>
		</div>
	</div>

	<div class="col-md-8 text-left">
		<label class="control-label" align="left"> <s:text
				name="IU1.8.3-4_LBL5" />
		</label>Cuevas Jimenez Beatriz
	</div>
	<fieldset class="form-section form-horizontal form-max">
		<s:set var="listAspirante"
			value="{{'Cuevas Jimenez Beatriz','06/03/2017', '7:00', '17'},{'Espino Maldonado Eduardo','06/03/2017', '7:15', '17'},{'Flores Sanchez Liliana','06/03/2017', '7:30', '17'},{'Jaramillo Loredo Fabiola','07/03/2017', '7:45', '18'},{'Larios Moguel Carlos Aníbal','07/03/2017', '8:00', '18'},{'Pascual Hernández Diego Efrén','07/03/2017', '8:15', '18'},{'Pérez Martínez Alan Alberto','07/03/2017', '8:30', '18'},{'Mendoza Santiago Irving','07/03/2017', '8:45', '18'}}" />
		<!-- entrevistador -->

		<table id="tblAspirantesProgramar"
			class="text-center table table-striped">
			<thead class="pull-center">
				<tr class="pull-center">
					<th data-priority="1"><s:text name="IU1.8.3-4_LBL1" /></th>
					<th data-priority="2"><s:text name="IU1.8.3-4_LBL2" /></th>
					<th data-priority="2"><s:text name="IU1.8.3-4_LBL3" /></th>
					<th data-priority="1"><s:text name="IU1.8.3-4_LBL4" /></th>
				</tr>
			</thead>
			<tbody>
				<s:iterator value="listAspirante" var="aspirante">
					<tr>
						<td>${aspirante[0]}</td>
						<td>${aspirante[1]}</td>
						<td>${aspirante[2]}</td>
						<td>${aspirante[3]}</td>
					</tr>
				</s:iterator>
			</tbody>
		</table>
	</fieldset>

	<!-- Botones de aceptar y cancelar -->
	<div class="form-group">
		<div class="row">
			<div class="text-right col-md-12">
				<a class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/entrevistas/gestionar-entrevistadores">
					<s:text name="btnRegresar" />
				</a>
			</div>
		</div>
	</div>
</body>
	</html>
</jsp:root>