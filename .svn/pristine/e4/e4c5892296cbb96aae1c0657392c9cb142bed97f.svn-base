<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<jsp:text>
	<![CDATA[                 
	<script type="text/javascript" src="${pageContext.request.contextPath}/pages/entrevistas/gestionar-entrevistadores/js/index.js"></script>
	]]>
</jsp:text>
</head>
<body>
	
	<!-- Evaluacion de la entrevista -->
	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.8.3.2_TITLE" />
			</h1>
		</div>
	</div>

	<!-- Tabla de entrevistadores -->
	<fieldset class="form-section">
		<div class="form-group">
			<div class="row">
				<div class="col-md-12">
					<table id="tblEntrevistadores" class="table table-striped">
						<thead>
							<tr>
								<th><s:text name="IU1.8.3.2_THEAD1" /></th>
								<th><s:text name="IU1.8.3.2_THEAD2" /></th>
							</tr>
						</thead>
						<tbody>
							<s:iterator value="listEntrevistadores" var="entrevistador">
								<tr>
									<td><s:property value="%{#entrevistador.entrevistador}" /></td>
									<td><a
										href="${pageContext.request.contextPath}/entrevistas/gestionar-horario/new">
											<i class="material-icons md-24 md-eld">date_range</i>
									</a>
									<a href="${pageContext.request.contextPath}/entrevistas/gestionar-lista-aspirantes/1">
											<i class="material-icons md-24 md-eld">insert_drive_file</i>
										</a>
									 <a onClick="abrirDialogEntrevistador()">
											<i class="material-icons md-24 md-eld">delete</i>
										</a></td>
								</tr>
							</s:iterator>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</fieldset>
	
	<div class="form-group">
		<div class="col-md-12">
			<a href="${pageContext.request.contextPath}/entrevistas/gestionar-entrevistadores/new" class="btn btn-default btn-default-eld"><s:text name="mx.com.eld.boton.agregar"/></a>
		</div>
	</div>

	<!-- Dialog de eliminacion -->
	<sj:dialog id="myclickdialog" autoOpen="false" modal="true"
		title="%{getText('mx.eld.dialog.title.eliminar')}">
		<div class="form-group">
			<div class="col-md-12">
				<s:text name="MSG21">
					<s:param>entrevistador</s:param>
				</s:text>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12 buttons-right text-right">
				<a onclick="cerrarDialogEntrevistador();" class="btn btn-default btn-default-eld">Aceptar</a>
				<a onclick="cerrarDialogEntrevistador();" class="btn btn-default btn-default-eld">Cancelar</a>
			</div>
		</div>
	</sj:dialog>
</body>
	</html>
</jsp:root>