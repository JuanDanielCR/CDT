						<td>${periodo[1]}</td>
						<td>${periodo[2]}</td>
						<td>${periodo[3]}</td>
						<td>${periodo[4]}</td>
						<td><a
							href="${pageContext.request.contextPath}/convocatoria/gestionar-criterio!agregarPeriodoEdit"><i
								class="material-icons md-24 md-eld">mode_edit</i></a> <a href="#"
								onclick="myclickDialog()">
								<i class="material-icons md-24 md-eld">delete</i>
							</a></td>

					</tr>
				</s:iterator>
			</tbody>
		</table>
	</div>

	<!-- boton agregar -->
	<div class="row">
		<div class="outter-section">
			<div class="col-md-12">
				<a class="btn btn-default btn-default-eld" id="btnAgregar"
