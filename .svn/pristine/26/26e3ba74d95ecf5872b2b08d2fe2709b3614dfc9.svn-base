<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<jsp:text>
	<![CDATA[
		<script
			src="${pageContext.request.contextPath}/pages/admision/aspirantes-entrevistar/js/index.js"
			type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<body>
	<!-- Titulo de la lista de aspirantes -->
	<div class="row-title">
		<h1 class="title">
			<s:text name="IU1.8.1-1_TITLE"></s:text>
		</h1>
		<br />
	</div>

	<div class="col-md-12">
		<div class="col-md-8 text-left">
			<label class="control-label" align="left"> <s:text
					name="IU1.9.1_LBL25"></s:text>
			</label>${convocatoriaSel.numeroAceptar}
		</div>
		<div class="form form-horizontal  col-md-12 form-group">
			<table id="tableAspirantesSeleccionados" class="table table-striped">
				<thead class="text-center ">
					<tr>
						<th data-priority="1">
							<div>
								<s:text name="IU1.9.1_LBL11"></s:text>
							</div>
						</th>
						<th data-priority="3">
							<div>
								<s:text name="IU1.9.1_LBL6"></s:text>
							</div>
						</th>
						<th data-priority="4">
							<div>
								<s:text name="IU1.9.1_LBL7"></s:text>
							</div>
						</th>
						<th data-priority="5">
							<div>
								<s:text name="IU1.9.1_LBL8"></s:text>
							</div>
						</th>
						<th data-priority="2">
							<div>
								<s:text name="IU1.9.1_LBL5"></s:text>
							</div>
						</th>
					</tr>
				</thead>
				<tbody>
					<s:iterator value="listConvocatoriaAspirante" var="aspirante">
						<tr>
							<td><s:property value="%{#aspirante.aspirante}" /></td>
							<td><s:property
									value="%{ConvocatoriaAspiranteBs.promedioSecundariaBachillerato(#aspirante)}" /></td>
							<td><s:property value="%{#aspirante.examenCeneval}" /></td>
							<td><s:property value="%{#aspirante.examenPsicometrico}" /></td>
							<td><s:property
									value="%{ConvocatoriaAspiranteBs.promedioGeneral(#aspirante)}" /></td>
						</tr>
					</s:iterator>
				</tbody>
			</table>
		</div>
		<div class="row">
			<div class="col-md-4 text-left">
				<s:text name="IU1.9.1_LBL14"></s:text>
				<br /> <br />
				<s:text name="IU1.8.1-2_LBL1"></s:text>
				<br />
				<s:text name="IU1.9.1_LBL16"></s:text>
				<br />
				<s:text name="IU1.8.1-2_LBL2"></s:text>
				<br /> <br />
				<s:text name="IU1.9.1_LBL20"></s:text>
			</div>
			<div class="col-md-8  text-right">
				<a class="btn btn-default btn-default-eld" onclick="AceptAlumnos()">
					<s:text name="mx.com.eld.boton.aceptar"></s:text>
				</a> <a class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/admision/aspirantes-entrevistar">
					<s:text name="mx.com.eld.boton.cancelar" />
				</a>
			</div>
		</div>
	</div>

	<!-- Informacion sobre la ponderacion de las calificaciones -->
	<sj:dialog id="dlgAceptAlumnos" modal="true"
		title="%{getText('IU1.8.1-1_LBL1')}" autoOpen="false"
		openTopics="showDlgAceptarAlumnos"
		closeTopics="closeDlgAceptarAlumnos" resizable="false"
		draggable="false">
		<s:form id="frmListEspera"
			action="%{#pagecontext.request.contextPath}/admision/aspirantes-entrevistar!notificarAspirantes"
			theme="simple" method="post">
			<div class="form-horizontal">
				<div class="form-group">
					<div class="col-md-12">
						<s:text name="MSG34"></s:text>
					</div>
				</div>
				<s:if test="%{esperados}">
					<div class="form-group">
						<div class="col-md-12 alert alert-warning">
							<s:text name="MSG75"></s:text>
						</div>
					</div>
				</s:if>
				<div class="form-group">
					<div class="col-md-12 text-right">
						<s:submit cssClass="btn btn-default btn-default-eld"
							value="%{getText('mx.com.eld.boton.aceptar')}" />
						<a id="btnDialogCancelAceptar"
							class="btn btn-default btn-default-eld"
							onclick="closeAceptAlumnos()"> <s:text
								name="mx.com.eld.boton.cancelar" />
						</a>
					</div>
				</div>
			</div>
		</s:form>
	</sj:dialog>
</body>
	</html>
</jsp:root>