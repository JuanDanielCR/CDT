<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
		<![CDATA[
	<script
		src="${pageContext.request.contextPath}/pages/calendario/gestionar-ciclo-actividad/js/index-actividadesLists.js"
		type="text/javascript"></script>
	]]>
	<!-- Actividad -->
	<div id="divActividadesList">
		<div class="form-group">
			<label class="col-md-4 control-label label-obligatorio" for=""><s:text
					name="IU1.1-8_TITLE" /></label>
			<div class="col-md-8">
				<s:select name="model.idActividad"
					headerKey="%{getText('COM.ELD.DEFAULT_HEADER_KEY')}"
