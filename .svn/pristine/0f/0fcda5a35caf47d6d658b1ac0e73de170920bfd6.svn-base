<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<jsp:text>
	<![CDATA[                 
	<script type="text/javascript" src="${pageContext.request.contextPath}/pages/calendario/gestionar-calendario-academico/js/index.js"></script>
	]]>
</jsp:text>
</head>
<body>
	<s:set var="listCalendario"
		value="{
		{'2015-2016','10/01/2015','13/01/2016'},
		{'2015-2016','11/01/2015','12/01/2016'}, 
		{'2016-2017','12/01/2016','13/01/2017'},
		{'2016-2017','13/01/2016','14/01/2017'},
		{'2016-2017','15/01/2016','16/01/2017'},
		{'2016-2017','17/01/2016','18/01/2017'},
		{'2016-2017','08/01/2016','19/01/2017'},
		{'2017-2018','20/01/2017','13/01/2018'},
		{'2017-2018','22/01/2017','23/01/2018'},
		{'2017-2018','23/01/2017','24/01/2018'},
		{'2018-2019','25/01/2018','26/01/2019'},
		{'2019-2020','27/01/2019','28/01/2020'},
		{'2020-2021','28/01/2020','30/01/2021'}}" />


	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.1-7_TITLE" />
			</h1>
		</div>
	</div>


	<div class="form form-horizontal form-max text-justify">
		
	
	
		<div class="form-group">
			<div class="row">
				<div class="col-md-12">
					<table id="tblCalendario" class="table table-striped">
						<thead>
							<tr>
								<th data-priority="1"><s:text name="IU1.1-7_LBL2" /></th>
								<th data-priority="3"><s:text name="IU1.1-7_LBL3" /></th>
								<th data-priority="4"><s:text name="IU1.1-7_LBL4" /></th>
								<th data-priority="2"><s:text name="IU1.1-7_LBL5" /></th>
							</tr>
						</thead>
						<tbody>
							<s:iterator value="listCalendario" var="calendario">
								<tr>
									<td>${calendario[0]}</td>
									<td>${calendario[1]}</td>
									<td>${calendario[2]}</td>
									<td><a
										href="${pageContext.request.contextPath}/calendario/gestionar-actividad">
											<i class="material-icons md-24 md-eld">visibility</i>
									</a></td>
								</tr>
							</s:iterator>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
	
	
	
</body>
	</html>
</jsp:root>