<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<!-- Head -->
<head>
<jsp:text>
	<![CDATA[
	<script
		src="${pageContext.request.contextPath}/pages/entrevistas/gestionar-lista-aspirantes/js/index-reprogramar.js"
		type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<!-- Body -->
<body>
	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.8.2.9_TITLE"></s:text>
			</h1>
		</div>
	</div>

	<fieldset class="form-section form-horizontal">
		<s:set var="listAspirante"
			value="{{'Cuevas Jimenez Beatriz','Nueva Entrevista'},{'Espino Maldonado Eduardo','Nueva Entrevista'},{'Flores Sanchez Liliana','Nueva Entrevista'},{'Jaramillo Loredo Fabiola','Nueva Entrevista'},{'Larios Moguel Carlos Aníbal','Nueva Entrevista'},{'Mendoza Santiago Irving','Nueva Entrevista'},{'Pascual Hernández Diego Efrén','Nueva Entrevista'},{'Pérez Martínez Alan Alberto','Nueva Entrevista'}}" />
		<table id="tblAspirantesReprogramar" class="table table-striped">
			<thead>
				<tr>
					<th><s:text name="IU1.8.2.8_LBL1" /></th>
					<th><s:text name="IU1.8.2.8_LBL2" /></th>
					<th><s:text name="IU1.8.2.8_LBL3" /></th>
				</tr>
			</thead>
			<tbody>
				<s:iterator value="listAspirante" var="aspirante">
					<tr>
						<td>${aspirante[0]}</td>
						<td>${aspirante[1]}</td>
						<td><a href="#" onClick="abrirDialogEntrevistador()">
								<i class="material-icons md-24 md-eld">delete</i>
							</a></td>
					</tr>
				</s:iterator>
			</tbody>
		</table>
	</fieldset>

	<!-- Botones de aceptar y cancelar -->
	<div class="form-group">
		<div class="row">
			<div class="text-right col-md-12">
				<a class="btn btn-default btn-default-eld" href="#"> <s:text
						name="btnAceptar" />
				</a>
				<sj:dialog id="myclickdialog2" autoOpen="false" modal="true"
					title="Cancelar Operación">

					<div class="row">
						<div class="col-md-12">
							<s:text name="IU1.2-3_JD1" />
						</div>
					</div>
					<!-- Botones de aceptar y cancelar -->
					<div class="row">
						<div class="text-right col-md-12">
							<a class="btn btn-default btn-default-eld"
								href="${pageContext.request.contextPath}/entrevistas/gestionar-lista-aspirantes!reprogramar">
								<s:text name="IU1.2-2C_LBL9" />
							</a> <a class="btn btn-default btn-default-eld"
								href="${pageContext.request.contextPath}/entrevistas/gestionar-lista-aspirantes!reprogramar">
								<s:text name="IU1.2-2C_LBL10" />
							</a>
						</div>
					</div>

				</sj:dialog>
				<sj:a href="#" openDialog="myclickdialog2"
					class="btn btn-default btn-default-eld">
					<s:text name="btnCancelar" />
				</sj:a>
			</div>
		</div>
	</div>
	<sj:dialog id="myclickdialog" autoOpen="false" modal="true"
		title="Eliminar Criterio">

		<div class="row">
			<div class="col-md-12">
				<s:text name="IU1.2-2C_LBL8" />
			</div>
		</div>
		<!-- Botones de aceptar y cancelar -->
		<div class="row">
			<div class="text-right col-md-12">
				<a class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/entrevistas/gestionar-lista-aspirantes!reprogramar">
					<s:text name="IU1.2-2C_LBL9" />
				</a> <a class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/entrevistas/gestionar-lista-aspirantes!reprogramar">
					<s:text name="IU1.2-2C_LBL10" />
				</a>
			</div>
		</div>

	</sj:dialog>
</body>
	</html>
</jsp:root>