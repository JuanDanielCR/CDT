<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<!-- Head -->
<jsp:text>
	<![CDATA[
		<script
			src="${pageContext.request.contextPath}/pages/entrevistas/gestionar-confirmacion-aspirante/js/index.js"
			type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<body>
	<s:set var="title" value="%{getText('IU1.8.2-10_TITLE')}" />
	<s:set var="txConfirmar" value = "%{getText('MSG27', {'Cuevas Jimenez Beatriz'})}"></s:set>
	<div class="row title">
		<div class="col-md-12">
			<h1>${title}</h1>
		</div>
	</div>

	<!-- Body -->
	<s:set var="listAspirantes"
		value="{{'Cuevas Jimenez Beatriz','09/03/2018','10:00', '55435465767'},
		{'Espino Maldonado Eduardo','09/03/2018','10:30','5525233456'},
		{'Flores Sanchez Liliana','09/03/2018','11:00','5533333333'},
		{'Jaramillo Loredo Fabiola','09/03/2018','11:30','5512345678'},
		{'Larios Moguel Carlos Anibal','09/03/2018','12:00','5527182818'},
		{'Mendoza Santiago Irving','09/03/2018','12:30','5567895200'}}" />
	<div class="form form-horizontal form-max">
		<div class="row">
			<div class="col-md-12">
				<table id="tblAspirantes" class="table table-striped">
					<thead>
						<tr>
							<th><s:text name="%{getText('IU1.8.2-10_LBL1')}"></s:text></th>
							<th><s:text name="%{getText('IU1.8.2-10_LBL2')}"></s:text></th>
							<th><s:text name="%{getText('IU1.8.2-10_LBL3')}"></s:text></th>
							<th><s:text name="IU1.8.2-10_LBL4"></s:text></th>
							<th><s:text name="IU1.8.2-10_LBL5"></s:text></th>
						</tr>
					</thead>
					<tbody>
						<s:iterator value="#listAspirantes" var="p">
							<tr>
								<td>${p[0]}</td>
								<td>${p[1]}</td>
								<td>${p[2]}</td>
								<td>${p[3]}</td>
								<td>
									<!-- Educational Background --> <a onClick="dialogBorrar()">
										<i class="material-icons  md-24 md-eld">check_circle</i>
									</a>
								</td>

							</tr>
						</s:iterator>
					</tbody>
				</table>
			</div>
		</div>
		<sj:dialog id="dlgAceptar" modal="true" title="Confirmar Asistencia"
			autoOpen="false" openTopics="showDlgGuardar"
			closeTopics="closeDlgAceptar" resizable="true" draggable="false">
			<div class="form-group">
				<div class="col-md-12">${txConfirmar}</div>
				<div class="form-group text-right">
					<div class="row">
						<div class="col-md-12 buttons-center">
							<sj:a id="btnDialogAceptar" onClick="cerrarMensajeAceptar()"
								class="btn btn-default btn-default-eld">
								<s:text name="btnAceptar" />
							</sj:a>
							<sj:a id="btnDialogCancelar" onClick="cerrarMensajeAceptar()"
								class="btn btn-default btn-default-eld">
								<s:text name="btnCancelar" />
							</sj:a>
						</div>
					</div>
				</div>
			</div>
		</sj:dialog>
	</div>
</body>
	</html>
</jsp:root>