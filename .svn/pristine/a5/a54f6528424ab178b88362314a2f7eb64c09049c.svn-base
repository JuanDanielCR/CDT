<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<!-- Head -->
<jsp:text>
	<![CDATA[
		<script
			src="${pageContext.request.contextPath}/pages/admision/aspirantes-aceptar/js/index-show.js"
			type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<body>
	<!-- VARIABLES -->
	<s:set var="valorDefault"
		value="'0'" />

	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.9.1_LBL9"></s:text>
			</h1>
		</div>
	</div>

	<div class="col-md-12">
		<div class="col-md-8 text-left">
			<label class="control-label"> <s:text name="IU1.9.1_LBL10"></s:text>
			</label>${numeroAspirantes}
		</div>
		<div class="form form-horizontal col-md-12 form-group">
			<table id="tableAspirantesSeleccionados" class="table table-striped">
				<thead class="text-center ">
					<tr>
						<th data-priority="1"><s:text name="IU1.9.1_LBL11" /></th>
						<th data-priority="6"><s:text name="IU1.9.1_LBL6" /></th>
						<th data-priority="5"><s:text name="IU1.9.1_LBL7" /></th>
						<th data-priority="4"><s:text name="IU1.9.1_LBL8" /></th>
						<th data-priority="3"><s:text name="IU1.9.1_LBL13" /></th>
						<th data-priority="2"><s:text name="IU1.9.1_LBL5" /></th>
						<th data-priority="1"><s:text name="IU1.1-7_LBL5" /></th>
					</tr>
				</thead>
				<tbody>
					<s:iterator value="listConvocatoriaAspirante" var="p">
						<s:iterator value="%{#p.aspirante.informacionEscolar}" var="l">
						<s:set var="promedioGeneral" value="%{(#p.promedioEntrevista+#l.promedio+#valorDefault+#valorDefault)/4}"/>
						</s:iterator>
						<tr>
							<td class="text-center">${p.aspirante.nombre}</td>
							<td class="text-center">
							<s:iterator value="%{#p.aspirante.informacionEscolar}" var="l">
								${l.promedio}
							</s:iterator>
							</td>
							<td class="text-center">${valorDefault}</td>
							<td class="text-center">${valorDefault}</td>
							<td class="text-center">${p.promedioEntrevista}</td>							
							<td class="text-center"><s:property value="getText(getText('mx.edu.eld.doubleFormat'),{#promedioGeneral})"/></td>
							<td class="text-center"><a onclick="cambiarEstado()"> <i
									class="material-icons md-24 md-eld">delete</i>
							</a></td>
						</tr>
					</s:iterator>
				</tbody>
			</table>
		</div>
		<div class="row">
			<div class="col-md-12 text-center">
				<a class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/admision/aspirantes-aceptar/new">
					<s:text name="IU1.4-2_BTNAGREGAR" />
				</a>
			</div>
		</div>
		<div class="row">
			<div class="col-md-4 text-left">
				<s:text name="IU1.9.1_LBL14" />
				<br /> <br />
				<s:text name="IU1.9.1_LBL15" />
				<br />
				<s:text name="IU1.9.1_LBL16" />
				<br />
				<s:text name="IU1.9.1_LBL17" />
				<br />
				<s:text name="IU1.9.1_LBL19" />
				<br /> <br />
				<s:text name="IU1.9.1_LBL18" />
			</div>
		</div>
		<div class="row">
			<div class="form-horizontal col-md-12 text-right">
				<a class="btn btn-default btn-default-eld"
					onclick="cambiarEstadoAcept()"> <s:text name="btnAceptar" />
				</a> <a
					href="${pageContext.request.contextPath}/entrevistas/gestionar-entrevistas"
					class="btn btn-default btn-default-eld"> <s:text
						name="mx.com.eld.boton.cancelar" />
				</a>
			</div>
		</div>
	</div>
	<!-- DIALOGOS -->
	<sj:dialog id="dlgAceptAlumnos" modal="true"
		title="%{getText('IU1.9.1_LBL27')}" autoOpen="false" resizable="false"
		openTopics="showDlgAceptarAlumnos"
		closeTopics="closeDlgAceptarAlumnos" draggable="false">
		<div class="form-horizontal col-md-12">
			<div class="form-group">
				<div class="col-md-12">
					<s:text name="MSG33"></s:text>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-12 text-right">
					<a
						href="${pageContext.request.contextPath}/entrevistas/gestionar-entrevistas"
						class="btn btn-default btn-default-eld"> <s:text
							name="mx.com.eld.boton.aceptar" />
					</a> <a id="btnDialogCancelAceptar"
						class="btn btn-default btn-default-eld"
						onclick="closeCambiarEstadoAcept()"> <s:text
							name="btnCancelar" />
					</a>
				</div>
			</div>
		</div>
	</sj:dialog>

	<sj:dialog id="dlgEstatusAlumno" modal="true"
		title="%{getText('IU1.9.1_LBL29')}" autoOpen="false" resizable="false"
		openTopics="showEstatusAlumnoCambiar"
		closeTopics="closeEstatusAlumnoCancel" draggable="false">
		<div class="form-horizontal col-md-12">
			<div class="form-group">
				<div class="col-md-12">
					<s:property
						value="getText('MSG11', {'Eduardo Espino Maldonado', 'no aceptado'})" />
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-12 text-right">
					<sj:a id="btnDialogAcept" class="btn btn-degault btn-default-eld">
                        Si
                    </sj:a>
					<a onclick="closeCambiarEstado()"
						class="btn btn-default btn-default-eld"><s:text
							name="mx.com.eld.boton.cancelar" /></a>
				</div>
			</div>
		</div>
	</sj:dialog>

</body>
	</html>
</jsp:root>