@Namespace("/ceneval")
@AllowedMethods({ RegistrarFolioAct.RESULTADOS, RegistrarFolioAct.SHOW_RESULTADOS })
public class RegistrarFolioAct extends ActionSupportCDT {
	/**
	 * 
	 */
