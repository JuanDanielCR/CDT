<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<jsp:text>
	<![CDATA[
	<script
		src="${pageContext.request.contextPath}/pages/admision/registrar-aspirante/js/index-editNew.js"
		type="text/javascript"></script>
		<script
			src="${pageContext.request.contextPath}/pages/admision/registrar-aspirante/js/medios-contacto.js"
			type="text/javascript"></script>
		<script type="text/javascript" src="${pageContext.request.contextPath}/pages/admision/gestionar-pasos-proceso/js/index-pasos-proceso.js"></script>
		<script	src="${pageContext.request.contextPath}/pages/admision/pagar-admision/js/index-editNew.js" type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<body>
	<s:url var="varUrlT" action="pagar-admision"
		includeContext="true" />
	<s:hidden id="urlTerm" value="%{#varUrlT}" />

	<div class="row">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.5_TITLE" />
			</h1>
		</div>
	</div>

	<!-- ############################ BARRA DE PASOS DEL PROCESO #################################### -->
	<s:set var="varPasoProceso" value="1" />
	<s:hidden id="idPasoProceso" value="%{#varPasoProceso}" />
	<s:set var="varEtapaProceso" value="1" />
	<s:hidden id="idEtapaProceso" value="%{#varEtapaProceso}" />
	<div class="col-md-8 col-md-offset-2">
		<s:include value="../gestionar-pasos-proceso/index-pasos-proceso.jsp" />
	</div>

	<div class="row">
		<div class="col-md-8 col-md-offset-2">
			<s:form id="frmPagoCondiciones"
				action="%{#pagecontext.request.contextPath}/admision/pagar-admision"
				theme="simple" method="post">

				<div class="row">
					<div class="form form-horizontal text-left">
						<div class="form-group">
							<div class="col-md-12">
								<s:text name="IU1.5_LBL1" />
							</div>
							<div class="col-md-12">
								<s:text name="IU1.5_LBL2" />
							</div>
							<div class="col-md-12">
								<s:text name="IU1.5_LBL3" />
							</div>
						</div>

						<div class="checkbox">
							<label>
								<input name="checkbox" id="checkterminos" type="checkbox" value="1"><s:text name="IU1.5_LBL4" />
								</input>
							</label>
						</div>
					</div>
				</div>

				<div class="row">
					<div class="row buttons col-md-12 text-right">
						<!--<s:submit id="btnAceptar"
							cssClass="btn btn-default btn-default-eld"
							value="%{getText('btnAceptar')}" />-->
						<a id="btnAceptar" class="btn btn-default btn-default-eld" href=""> <s:text
								name="btnAceptar" />
						</a>
					</div>
				</div>
			</s:form>
		</div>
	</div>
</body>
	</html>
</jsp:root>