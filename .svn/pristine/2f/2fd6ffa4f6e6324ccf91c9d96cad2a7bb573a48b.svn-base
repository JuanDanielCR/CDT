<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<jsp:text>
	<![CDATA[
		<script
			src="${pageContext.request.contextPath}/pages/admision/aspirantes-entrevistar/js/index-editNew.js"
			type="text/javascript"></script>
	]]>
</jsp:text>
<!-- Head -->
</head>
<body>

	<s:set var="listAspirantes"
		value="{{'Pancho de los Angeles Ramirez Soto','9','8.7','8.2','8.1'},
		{'Salinas Abraham Hernandez EncarnaciÃ³n','9','6.7','8.2','8.12'},
		{'Alma Dulce Hernandez Perez','9','8.7','8.2','8.42'},
		{'Maria del Rosario Salgado Estrada','7.7','8.7','8.2','9.12'}}" />



	<!-- Titulo de la lista de aspirantes -->
	<div class="row-title">
		<h1 class="title">
			<s:text name="IU1.8.1-2_LBL3"></s:text>
		</h1>
		<br />
	</div>
	<div class="col-md-12">
		<div class="col-md-8 text-left">

			<h3>
				<s:text name="IU1.8.1-2_LBL5"></s:text>
			</h3>
			<label class="control-label" align="left"> <s:text
					name="IU1.8.1-2_LBL4"></s:text>
			</label>${convocatoriaSel.numeroEntrevistar}&#43;
		</div>
		<s:form id="frmListEspera"
			action="%{#pagecontext.request.contextPath}/admision/aspirantes-entrevistar/%{convocatoriaSel.id}"
			theme="simple" method="post">
			<s:hidden id="_method" name="_method" value="PUT" />
			<div class="form form-horizontal  col-md-12 form-group">
				<table id="tableAspirantesSeleccionados" class="table table-striped">
					<thead class="text-center ">
						<tr>
							<th data-priority="1">
								<div>
									<s:text name="IU1.9.1_LBL11"></s:text>
								</div>
							</th>
							<th data-priority="3">
								<div>
									<s:text name="IU1.9.1_LBL6"></s:text>
								</div>
							</th>
							<th data-priority="4">
								<div>
									<s:text name="IU1.9.1_LBL7"></s:text>
								</div>
							</th>
							<th data-priority="5">
								<div>
									<s:text name="IU1.9.1_LBL8"></s:text>
								</div>
							</th>
							<th data-priority="2">
								<div>
									<s:text name="IU1.9.1_LBL5"></s:text>
								</div>
							</th>
							<th data-priority="1">
								<div>
									<s:text name="IU1.4-1_LBL4"></s:text>
								</div>
							</th>
						</tr>
					</thead>
					<tbody>
						<s:iterator value="listConvocatoriaAspirante" var="c">
							<tr>
								<td><s:property value="%{#c.aspirante}" /></td>
								<td><s:property
										value="%{ConvocatoriaAspiranteBs.promedioSecundariaBachillerato(#c)}" /></td>
								<td><s:property value="%{#c.examenCeneval}" /></td>
								<td><s:property value="%{#c.examenPsicometrico}" /></td>
								<td><s:property
										value="%{ConvocatoriaAspiranteBs.promedioGeneral(#c)}" /></td>
								<td><input id="chk_${c.idAspirante}"
									type="checkbox" name="listAspirantes"
									value="${c.idAspirante}"></input></td>
							</tr>
						</s:iterator>
					</tbody>
				</table>
			</div>
			<div class="row">
				<div class="row buttons col-md-12 text-right">
					<s:submit cssClass="btn btn-default btn-default-eld"
						value="%{getText('mx.com.eld.boton.aceptar')}" />
					<a class="btn btn-default btn-default-eld"
						href="${pageContext.request.contextPath}/admision/aspirantes-entrevistar/new">
						<s:text name="mx.com.eld.boton.cancelar" />
					</a>
				</div>
			</div>
		</s:form>
	</div>

	<!-- DIALOGOS -->

	<sj:dialog id="dlgAceptAlumnos" modal="true"
		title="%{getText('IU1.8.1-1_LBL1')}" autoOpen="false"
		resizable="false" openTopics="showDlgAceptarAlumnos"
		closeTopics="closeDlgAceptarAlumnos" draggable="false">
		<div class="form-horizontal">
			<div class="form-group">
				<div class="col-md-12">
					<s:text name="MSG34"></s:text>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-12 text-right">
					<sj:a id="btnDialogAceptAceptar"
						class="btn btn-degault btn-default-eld">
						<s:text name="mx.com.eld.boton.aceptar" />
					</sj:a>
					<a id="btnDialogCancelAceptar"
						class="btn btn-default btn-default-eld"
						onclick="closeAceptAlumnos()"> <s:text
							name="mx.com.eld.boton.cancelar" />
					</a>
				</div>
			</div>
		</div>
	</sj:dialog>

</body>
	</html>
</jsp:root>