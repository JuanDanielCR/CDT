<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<!-- Head -->
<jsp:text>
	<![CDATA[
		<script
			src="${pageContext.request.contextPath}/pages/entrevistas/gestionar-salones/js/index-editNew.js"
			type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<body>
	<s:set var="txGuardo"
		value="%{getText('MSG1_3',{'lista de los salones para las entrevistas', 'guardó'})}" />
	<s:set var="txCancelar" value="%{getText('MSG2')}" />
	<s:set var="title" value="%{getText('IU1.8.2-1_TITLE')}" />
	<s:set var="listSalones"
		value="{{'Salón 1','1'},{'Salón 2','2'},{'Salón 3','3'},{'Salón 4','4'},{'Salón 5','5'},{'Salón 6','5'},{'Salón 7','7'},{'Salón 8','11'}}" />
	<div class="row title">
		<div class="col-md-12">
			<h1>${title}</h1>
		</div>
	</div>
	<!-- Body -->

	<div class="form form-horizontal text-justify">
		<table id="tblSalones" class="table table-striped">
			<thead>
				<tr>
					<th data-priority="1"><s:text
							name="%{getText('IU1.8.2-1_LBL1')}"></s:text></th>
					<th data-priority="3"><s:text
							name="%{getText('IU1.8.2-1_LBL3')}"></s:text></th>
					<th data-priority="2"><s:text
							name="%{getText('IU1.8.2-1_LBL2')}"></s:text></th>
				</tr>
			</thead>
			<tbody>
				<s:iterator value="#listSalones" var="p">
					<tr>
						<td class="text-left">${p[0]}</td>
						<td class="text-left">${p[1]}</td>
						<td><s:checkbox name="agregar"></s:checkbox></td>
					</tr>
				</s:iterator>
			</tbody>
		</table>
	</div>


	<div class="row">
		<div class="outter-section form-medium">
			<div class="col-md-12 text-right">
				<sj:a class="btn btn-default btn-default-eld">
					<s:text name="mx.com.eld.boton.aceptar" />
				</sj:a>
				<sj:a class="btn btn-default btn-default-eld">
					<s:text name="mx.com.eld.boton.cancelar" />
				</sj:a>
			</div>
		</div>
	</div>


</body>
	</html>
</jsp:root>