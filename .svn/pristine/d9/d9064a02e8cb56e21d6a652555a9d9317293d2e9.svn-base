<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
    <jsp:directive.page language="java"
                        contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"/>
    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <!-- Head -->
        <jsp:text>
            <![CDATA[
		<script
			src="${pageContext.request.contextPath}/pages/admision/gestionar-aspirantes/js/index.js"
			type="text/javascript"></script>
	]]>
        </jsp:text>

    </head>
    <body>

    <s:set var="title" value="%{getText('IU1.4-1_TITLE')}"/>

    <div class="form-group">
        <div class="row">
            <div class="col-md-12">
                <h1>${title}</h1>
            </div>
        </div>
    </div>

    <div class="outter-section form-horizontal form-max">
        <div class="row">
            <div class="col-md-12">
                <s:actionerror theme="%{getText('mx.edu.eld.defaulTheme')}"/>
                <s:actionmessage theme="%{getText('mx.edu.eld.defaulTheme')}"/>
            </div>
        </div>
    </div>

    <!-- Body -->
    <div class="form form-horizontal form-max">
        <div class="row">
            <div class="col-md-12">
                <table id="tblAspirantes" class="table table-striped">
                    <thead>
                    <tr>
                        <th><s:text name="IU1.4-1_LBL5"/></th>
                        <th><s:text name="IU1.4-1_LBL1"/></th>
                        <th><s:text name="IU1.4-1_LBL2"/></th>
                        <th><s:text name="IU1.4-1_LBL4"/></th>
                    </tr>

                    </thead>

                    <tbody>
                    <s:iterator value="listAspirante" var="p">
                        <tr>
                            <td>${p.nombre} ${p.primerApellido} ${p.segundoApellido} <!-- <s:if test="#p. eq 'Incompleto'">
										<i class="material-icons md-24 md-eld">fiber_manual_record</i>${p[}</s:if>
									<s:else>
										<i class="material-icons  md-24 md-eld-green ">fiber_manual_record</i>${p[0]}<label
											class="pull-right"><i
											class="material-icons md-24 md-eld-green">attach_money</i></label>
									</s:else> -->
                            </td>
                            <td>${p.curp}</td>
                            <td></td>
                            <td>
                                <!-- Educational Background --> <a
                                    href="${pageContext.request.contextPath}/admision/gestionar-aspirantes!registrarAspirante?idAspiranteSel=${p.id}">
                                <i class="material-icons  md-24 md-eld">border_color</i> <!--	title="${tltEducationBackground}"
										alt="${tltEducationBackground}" -->
                            </a>
                            </td>
                        </tr>
                    </s:iterator>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-md-12 text-center">
                <a id="btnRegistrarAspirante"
                   class="btn btn-default btn-default-eld"
                   href="${pageContext.request.contextPath}/admision/registrar-aspirante"><s:text
                        name="btnRegistrarAspirante"/></a>
            </div>
        </div>
    </div>
    </body>
    </html>
</jsp:root>