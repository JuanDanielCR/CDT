<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
</head>
<body>
	<!-- VARIABLES -->
	<s:set var="btnAceptar" value="getText('btnAceptar')" />
	<!-- BODY -->
	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.6.4_TITLE" />
			</h1>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12 text-left">
			<span><s:text name="camposObligatorios" /></span>
		</div>
	</div>
	<s:form>
		<fieldset class="form-section form-horizontal">
			<legend class="form-section">
				<s:text name="IU1.6.4_LBL1" />
			</legend>
			<div class="form-group">
				<label class="col-md-6 control-label label-obligatorio"><s:text
						name="IU1.6.4_LBL2" /></label>
				<div class="col-md-2">
					<sj:spinner cssClass="form-control" min="0" max="100" value="0" />
				</div>
				<div class="col-md-offset-4"></div>
			</div>
		</fieldset>
	</s:form>
	<div class="row">
		<div class="col-md-12 text-right">
			<!--<s:submit cssClass="btn btn-default btn-default-eld" value="%{#btnAceptar}" /> -->
			<a class="btn btn-default btn-default-eld"
				href="${pageContext.request.contextPath}/ceneval/planificar-salones"><s:text
					name="btnAceptar" /></a> <a class="btn btn-default btn-default-eld"><s:text
					name="btnCancelar" /></a>
		</div>
	</div>
</body>
	</html>
</jsp:root>