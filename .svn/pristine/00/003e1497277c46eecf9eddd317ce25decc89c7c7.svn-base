<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">

<head>
<!-- Head -->
<jsp:text>
	<![CDATA[
	<script
		src="${pageContext.request.contextPath}/pages/admision/registrar-aspirante/js/index-editNew.js"
		type="text/javascript"></script>
		<script
			src="${pageContext.request.contextPath}/pages/admision/registrar-aspirante/js/medios-contacto.js"
			type="text/javascript"></script>
		<script type="text/javascript" src="${pageContext.request.contextPath}/pages/admision/gestionar-pasos-proceso/js/index-pasos-proceso.js"></script>
	]]>
</jsp:text>
</head>
<body>
	<!-- Body -->
	<s:set var="title" value="%{getText('IU1.4-5_TITLE')}" />
	<s:set var="btnSi" value="%{getText('IU1.4-2_BTNSI')}" />
	<s:set var="phArchivo" value="%{getText('LBL_ARCHIVO')}" />
	<div class="row">
		<div class="col-md-12">
			<h1 class="title">${title}</h1>
		</div>
	</div>

	<!-- ############################ BARRA DE PASOS DEL PROCESO #################################### -->
	<s:set var="varPasoProceso" value="0" />
	<s:hidden id="idPasoProceso" value="%{#varPasoProceso}" />
	<s:set var="varEtapaProceso" value="1" />
	<s:hidden id="idEtapaProceso" value="%{#varEtapaProceso}" />
	<div>
		<s:include value="../gestionar-pasos-proceso/index-pasos-proceso.jsp" />
	</div>
	<div class="form-group text-left">
		<s:text name="IU1.4-2_LBL1" />
	</div>
	<fieldset class="form-section form-horizontal">
		<s:actionerror theme="%{getText('mx.edu.eld.defaulTheme')}" />
		<legend class="form-section">
			<s:text name="IU1.4-5_TITLE" />
		</legend>
		<!--
	<s:set var="listTiposContacto"
		value="{'1':'Teléfono fijo', '2':'Teléfono fijo', '3':'Correo electrónico'}" />
	<s:set var="listCategoriasContacto"
		value="{{'Trabajo',2}, {'Personal',1}, {'Contacto de emergencia',0}}" /> !-->
		<s:set var="listRespuestaAdeudo" value="{{'Sí',1}, {'No',0}}" />
		<s:set var="listContactosSel"
			value="{{'Rodrigo','228143533','123',0}}" />
		<s:set var="listEscuela" value="{{'Oficial',1}, {'Particular',0}}" />
		<!-- Body -->
		<div>
			<div class="form-group">
				<div id="divGlobalErrors" class="col-md-12 col-contact">
					<s:fielderror id="hdnGeneral" fieldName="model.globalHidden"
						cssClass="error" theme="%{getText('mx.edu.eld.defaulTheme')}" />
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-4 control-label label-obligatorio" for=""><s:text
						name="IU1.4-5_LBL8" /></label>
				<div class="col-md-8 col-contact">
					<s:select id="slcListTipoContactos" list="listTiposContacto"
						name="model.idTipo" listValue="nombre" listKey="id" headerKey="-1"
						headerValue="Select Tipo" cssClass="form-control" required="true"
						cssErrorClass="error" />
				</div>
			</div>

			<div class="form-group">
				<label class="col-md-4 control-label" for=""><s:text
						name="IU1.4-5_LBL9" /></label>
				<div class="col-md-8 col-xs-12 radio-control">
					<div class="col-md-8 form-control-radio pull-left">
						<s:set var="varChecked" value="" />
						<s:iterator value="listCategorias" var="g">
							<span class="col-md-4 col-xs-4 radio-container"> <s:if
									test="%{#g.id eq model.idCategoria}">
									<input id="categoria_${g.id}" name="model.idCategoria"
										type="radio" value="${g.id}" onclick="seleccionarCategoria()"
										checked="checked" />
								</s:if> <s:else>
									<input id="categoria_${g.id}" name="model.idCategoria"
										type="radio" value="${g.id}" onclick="seleccionarCategoria()" 
										/>
								</s:else> 
								${g.nombre}
							</span>
						</s:iterator>
						<div id="divCategoryErrors" class="col-md-12 col-xs-12 col-contact">
							<s:fielderror id="hdnCategoria" fieldName="model.categoriaHidden"
								cssClass="error" theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
				</div>
			</div>
			<s:form id="frmContactoTelefono"
				action="%{#pageContext.request.contextPath}/admision/gestionar-contactos"
				theme="simple" method="post">
				<div id="divTelefono" class="form-group hidden">
					<s:hidden id="hdnIdContactoTelefono" name="model.id" />
					<s:hidden id="hdnIdTipoContactoTelefono" name="model.idTipo" />
					<s:hidden id="hdnIdCategoriaTelefono" name="model.idCategoria" />
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-5_LBL10" /></label>
						<div class="col-md-8 col-contact">
							<s:textfield id="txTelephoneCodeArea" name="model.contactoA"
								type="text" cssClass="form-control" placeholder="" maxlength="3" />
							<s:fielderror fieldName="model.contactoA" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-5_LBL11" /></label>
						<div class="col-md-8 col-contact">
							<s:textfield id="txTelephone" name="model.contacto" type="text"
								cssClass="form-control" placeholder="" maxlength="8" />
							<s:fielderror fieldName="model.contacto" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label" for=""><s:text
								name="IU1.4-5_LBL12" /></label>
						<div class="col-md-8 col-contact">
							<s:textfield id="txTelephoneExtension" name="model.contactoB"
								type="text" cssClass="form-control" placeholder="" maxlength="5" />
							<s:fielderror fieldName="model.contactoB" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
					<div id="divEmergenciaTelefono" class="form-group hidden">
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-5_LBL26" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txEmergenciaTelefono" name="model.emergencia"
									type="text" cssClass="form-control" placeholder=""
									maxlength="100" />
								<s:fielderror fieldName="model.emergencia" cssClass="error"
									theme="%{getText('mx.edu.eld.defaulTheme')}" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-5_LBL27" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txRelationTelefono" name="model.relacion"
									type="text" cssClass="form-control" placeholder=""
									maxlength="50" />
								<s:fielderror fieldName="model.relacion" cssClass="error"
									theme="%{getText('mx.edu.eld.defaulTheme')}" />
							</div>
						</div>
					</div>
				</div>
			</s:form>
			<s:form id="frmContactoMovil"
				action="%{#pageContext.request.contextPath}/admision/gestionar-contactos"
				theme="simple" method="post">
				<div id="divMovil" class="form-group hidden">
					<s:hidden id="hdnIdContactoMovil" name="model.id" />
					<s:hidden id="hdnIdTipoContactoMovil" name="model.idTipo" />
					<s:hidden id="hdnIdCategoriaMovil" name="model.idCategoria" />
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-5_LBL10" /></label>
						<div class="col-md-8 col-contact">
							<s:textfield id="txMobileCodeArea" name="model.contactoA"
								type="text" cssClass="form-control" placeholder="" maxlength="3" />
							<s:fielderror fieldName="model.contactoA" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-5_LBL28" /></label>
						<div class="col-md-8 col-contact">
							<s:textfield id="txMobileTelephone" name="model.contacto"
								type="text" cssClass="form-control" placeholder=""
								maxlength="10" />
							<s:fielderror fieldName="model.contacto" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
					<div id="divEmergenciaMovil" class="form-group hidden">
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-5_LBL26" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txEmergenciaMovil" name="model.emergencia"
									type="text" cssClass="form-control" placeholder=""
									maxlength="100" />
								<s:fielderror fieldName="model.emergencia" cssClass="error"
									theme="%{getText('mx.edu.eld.defaulTheme')}" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-5_LBL27" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txRelationMovil" name="model.relacion"
									type="text" cssClass="form-control" placeholder=""
									maxlength="50" />
								<s:fielderror fieldName="model.relacion" cssClass="error"
									theme="%{getText('mx.edu.eld.defaulTheme')}" />
							</div>
						</div>
					</div>
				</div>
			</s:form>
			<s:form id="frmContactoEmail"
				action="%{#pageContext.request.contextPath}/admision/gestionar-contactos"
				theme="simple" method="post">
				<div id="divEmail" class="form-group hidden">
					<s:hidden id="hdnIdContactoEmail" name="model.id" />
					<s:hidden id="hdnIdTipoContactoEmail" name="model.idTipo" />
					<s:hidden id="hdnIdCategoriaEmail" name="model.idCategoria" />
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-5_LBL29" /></label>
						<div class="col-md-8 col-contact">
							<s:textfield id="txEmail" name="model.contacto" type="text"
								cssClass="form-control" placeholder="" maxlength="50" />
							<s:fielderror fieldName="model.contacto" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
						</div>
					</div>
					<div id="divEmergenciaEmail" class="form-group hidden">
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-5_LBL26" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txEmergenciaEmail" name="model.emergencia"
									type="text" cssClass="form-control" placeholder=""
									maxlength="100" />
								<s:fielderror fieldName="model.emergencia" cssClass="error"
									theme="%{getText('mx.edu.eld.defaulTheme')}" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-5_LBL27" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txRelationEmail" name="model.relacion"
									type="text" cssClass="form-control" placeholder=""
									maxlength="50" />
								<s:fielderror fieldName="model.relacion" cssClass="error"
									theme="%{getText('mx.edu.eld.defaulTheme')}" />
							</div>
						</div>
					</div>
				</div>
				<!-- Boton que llama al dialog, se llama con medios-contacto.js -->
				<div class="row">
					<div class="form-group">
						<div class="col-md-12">
							<a id="btnAgregar" class="btn btn-default btn-default-eld"
								onClick="agregar()"><s:text name="IU1.4-2_BTNAGREGAR" /></a>
						</div>
					</div>
				</div>
			</s:form>
		</div>
		<!-- Lista de contactos -->
		<div class="form-group">
			<div class="row text-center">
				<div id="divContactos" class="col-md-12 text-center">
					<div class="contactos-container">
						<s:fielderror fieldName="contactoFieldError"
							theme="%{getText('mx.edu.eld.defaulTheme')}" />
						<s:if test="%{#listContactosSel.size > 0}">
							<s:set var="tltEdit" value="%{getText('COM.SPRACH.TLT_EDITAR')}" />
							<s:set var="tltDelete"
								value="%{getText('COM.SPRACH.TLT_DELETE')}" />
							<table id="tblContactos">
								<s:iterator value="listContactos" var="c">
									<tr>
										<td>${c}</td>
										<td class="col-buttons"><i
											class="material-icons md-24 md-eld" title="Delete"
											alt="Delete" onclick="eliminarContacto(${c.id})">delete</i></td>
									</tr>
								</s:iterator>
							</table>
						</s:if>
						<s:else>
							<div class="contact-text">
								<s:text name="Aún no se han agregado contactos" />
							</div>
						</s:else>
					</div>
				</div>
			</div>
		</div>
		<!-- PENDIENTE ACABAR MEDIOS DE CONTACTOS -->
	</fieldset>
	<div class="form-group">
		<div class="row">
			<div class="col-md-12 text-right">
				<a id="btnAceptar" class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/admision/gestionar-contactos!validaMinimoContactos"><s:text
						name="btnAceptar" /></a> <a id="btnCancelar"
					class="btn btn-default btn-default-eld"
					href="${pageContext.request.contextPath}/admision/registrar-aspirante"><s:text
						name="btnCancelar" /></a>
			</div>
		</div>
	</div>

	<!-- DIALOGOS -->

	<sj:dialog id="dlgDeleteContact" modal="true"
		title="Eliminar Medio de Contacto" autoOpen="false"
		openTopics="showDlgDeleteContact" closeTopics="closeDlgDeleteContact"
		resizable="true" draggable="false">
		<s:url var="urlAction" action="gestionar-contactos/"
			includeContext="true" />
		<s:hidden id="hdnIdContactoDelete" value="%{#urlAction}" />
		<div class="form-horizontal">
			<s:form id="frmDeleteContacto"
				action="%{#pageContext.request.contextPath}/admision/gestionar-contactos/%{#hdnIdContactoDelete}"
				method="post">
				<s:hidden name="_method" value="delete"></s:hidden>
				<div class="form-group">
					<div class="col-md-12 col-contact">
						<s:text name="IU1.4-3_MSG_ELIMINAR" />
					</div>
				</div>
				<div class="form-group">
					<div class="col-md-12 text-right">
						<s:submit id="btnDialogDeleteAceptar"
							class="btn btn-default btn-default-eld" title="%{#btnSi}"
							value="%{#btnSi}">
						</s:submit>
						<a id="btnDialogDeleteCancelar"
							class="btn btn-default btn-default-eld"><s:text name="No" /></a>
					</div>
				</div>
			</s:form>
		</div>
	</sj:dialog>
</body>

	</html>
</jsp:root>