<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<jsp:text>
	<![CDATA[
		<script type="text/javascript" src="${pageContext.request.contextPath}/pages/admision/gestionar-pasos-proceso/js/index-pasos-proceso.js"></script>
		]]>
</jsp:text>
</head>
<body>
	<!-- VARIABLES -->
	<s:set
		value="{'093567126','JALF930110MDFRRB01','Fabiola Jaramillo Loredo','Aprobado'}"
		var="resultado" />
	<s:set var="groupName" value="''" />
	<!-- BODY -->
	<div class="row title">
		<div class="col-md-12">
			<h1 class="title">
				<s:text name="IU1.6-10_TITLE" />
			</h1>
		</div>
	</div>

	<!-- ############################ BARRA DE PASOS DEL PROCESO #################################### -->
	<s:set var="varPasoProceso" value="2" />
	<s:hidden id="idPasoProceso" value="%{#varPasoProceso}" />
	<s:set var="varEtapaProceso" value="1" />
	<s:hidden id="idEtapaProceso" value="%{#varEtapaProceso}" />
	<div class="col-md-8 col-md-offset-2">
		<s:include
			value="../../admision/gestionar-pasos-proceso/index-pasos-proceso.jsp" />
	</div>

	<div class="col-md-8 col-md-offset-2">
		<div class="col-md-12">
			<h4 class="text-left">
				<s:text name="IU1.6-10_LBL1" />
			</h4>
		</div>
		<s:form>
			<fieldset class="form-section form-horizontal">
				<legend class="form-section">
					<s:property value="%{#groupName}" />
				</legend>
				<div class="form-group">
					<label class="col-md-6 control-label"><s:text
							name="IU1.6-10_LBL2" /></label>
					<div class="col-md-6 text-left">
						<label><s:property value="%{#resultado[0]}" /></label>
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-6 control-label"><s:text
							name="IU1.6-10_LBL3" /></label>
					<div class="col-md-6 text-left">
						<label><s:property value="%{#resultado[1]}" /></label>
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-6 control-label"><s:text
							name="IU1.6-10_LBL4" /></label>
					<div class="col-md-6 text-left">
						<label><s:property value="%{#resultado[2]}" /></label>
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-6 control-label"><s:text
							name="IU1.6-10_LBL5" /></label>
					<div class="col-md-6 text-left">
						<label><s:property value="%{#resultado[3]}" /></label>
					</div>
				</div>
			</fieldset>
		</s:form>
	</div>
	<div class="row">
		<div class="col-md-12 buttons text-center">
			<a
				href="${pageContext.request.contextPath}/ceneval/gestionar-examen/new?success=1"
				class="btn btn-default btn-default-eld"><s:text
					name="btnAceptar" /></a>
		</div>
	</div>
</body>
	</html>
</jsp:root>