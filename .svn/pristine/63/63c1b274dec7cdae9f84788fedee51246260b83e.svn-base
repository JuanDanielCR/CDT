<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">

<!-- Evaluacion de la entrevista -->
<div class="row title">
	<div class="col-md-12">
		<h1 class="title">
			<s:text name="IU1.8.5-1_TITLE" />
		</h1>
	</div>
</div>

<!-- Tabla de entrevistas -->
<div class="form-section form-horizontal">
	<div class="form-group">
		<div class="row">
			<div class="col-md-12">
				<table id="tblEntrevistas" class="table table-striped">
					<thead>
						<tr>
							<th><s:text name="IU1.8.5-1_THEAD1" /></th>
							<th><s:text name="IU1.8.5-1_THEAD2" /></th>
						</tr>
					</thead>
					<tbody>
						<s:iterator value="listEntrevistas" var="entrevista">
							<tr>
								<td>${entrevista[0]}</td>
								<td><a
									href="${pageContext.request.contextPath}/entrevistas/gestionar-entrevistas/new">
										<i class="material-icons md-24 md-eld">&#xE14F;</i>
								</a></td>
							</tr>
						</s:iterator>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
	</html>
</jsp:root>