<?xml version="1.0" encoding="UTF-8" ?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
	xmlns:s="/struts-tags" xmlns:sj="/struts-jquery-tags">
	<jsp:directive.page language="java"
		contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<!-- Head -->
<jsp:text>
	<![CDATA[
	<script
		src="${pageContext.request.contextPath}/pages/admision/registrar-aspirante/js/index-editNew.js"
		type="text/javascript"></script>
		<script
			src="${pageContext.request.contextPath}/pages/admision/registrar-aspirante/js/medios-contacto.js"
			type="text/javascript"></script>
	]]>
</jsp:text>
</head>
<body>

	<s:set var="listTiposContacto"
		value="{'1':'Teléfono fijo', '2':'Teléfono fijo', '3':'Correo electrónico'}" />
	<s:set var="listCategoriasContacto"
		value="{{'Trabajo',2}, {'Personal',1}, {'Contacto de emergencia',0}}" />
	<s:set var="listRespuestaAdeudo" value="{{'Sí',1}, {'No',0}}" />
	<s:set var="listContactosSel" value="{{'Rodrigo','228143533','123',0}}" />
	<s:set var="listEscuela" value="{{'Oficial',1}, {'Particular',0}}" />

	<!-- Body -->
	<s:set var="title" value="%{getText('IU1.4-2_TITLE')}" />
	<s:set var="phArchivo" value="%{getText('LBL_ARCHIVO')}" />

	<div class="row">
		<div class="col-md-12">
			<h1>${title}</h1>
		</div>
	</div>


	<div class="form form-horizontal form-max">
		<s:actionerror theme="%{getText('mx.edu.eld.defaulTheme')}" />
		<div class="form-group text-left">
			<s:text name="IU1.4-2_LBL1" />
		</div>

		<sj:tabbedpanel id="localtabs">
			<sj:tab id="tab1" target="datosPersonales" label="Datos Personales" />
			<sj:tab id="tab2" target="contactos" label="Medios de Contacto" />
			<sj:tab id="tab3" target="domicilio" label="Domicilio" />
			<sj:tab id="tab4" target="informacionEscolar"
				label="Información Escolar" />
			<div id="datosPersonales">
				<s:form id="frmDatosPersonales"
					action="%{#pageContext.request.contextPath}/" theme="simple"
					method="post">
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-2_LBL2" /></label>
						<div class="col-md-8">
							<s:textfield id="txNombre" name="model.nombre" type="text"
								cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL2')}"
								maxlength="20" cssErrorClass="field-error" />
							<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-2_LBL3" /></label>
						<div class="col-md-8">
							<s:textfield id="txPrimerApellido" name="model.primerApellido"
								type="text" cssClass="form-control"
								placeholder="%{getText('IU1.4-2_LBL3')}" maxlength="20"
								cssErrorClass="field-error" />
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-2_LBL4" /></label>
						<div class="col-md-8">
							<s:textfield id="txSegundoApellido" name="model.segundoApellido"
								type="text" cssClass="form-control"
								placeholder="%{getText('IU1.4-2_LBL4')}" maxlength="20"
								cssErrorClass="field-error" />
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-2_LBL5" /></label>
						<div class="col-md-8 pull-left">
							<span id="identificacionOficial" class="col-md-4" for=""><s:text
									name="LBL_ARCHIVO" /></span>
							<div class="input-group col-md-8">
								<input type="hidden" class="form-control" /> <span
									class="input-group-btn pull-left" id="loadImage"> <i
									class="material-icons md-24 md-eld"
									onclick="selectImage('fileIdentificacion');">file_upload</i>
								</span> <input type='file' id="fileIdentificacion"
									name="fileIdentificacion" class="hide" accept=".pdf" value="" />
							</div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-2_LBL6" /></label>
						<div class="col-md-8 pull-left">
							<span id="actaNacimiento" class="col-md-4" for=""><s:text
									name="LBL_ARCHIVO" /></span>
							<div class="input-group col-md-8">
								<input type="hidden" class="form-control" /> <span
									class="input-group-btn pull-left" id="loadImage"> <i
									class="material-icons md-24 md-eld"
									onclick="selectImage('fileActaNacimiento');">file_upload</i>
								</span> <input type='file' id="fileActaNacimiento"
									name="fileActaNacimiento" class="hide" accept=".pdf" value="" />
							</div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-4 control-label label-obligatorio" for=""><s:text
								name="IU1.4-2_LBL7" /></label>
						<div class="col-md-8 pull-left">
							<span id="curp" class="col-md-4" for=""><s:text
									name="LBL_ARCHIVO" /></span>
							<div class="input-group col-md-8">
								<input type="hidden" class="form-control" /> <span
									class="input-group-btn pull-left" id="loadImage"> <i
									class="material-icons md-24 md-eld"
									onclick="selectImage('fileCurp');">file_upload</i>
								</span> <input type='file' id="fileCurp" name="fileCurp" class="hide"
									accept=".pdf" value="" />
							</div>
						</div>
					</div>
				</s:form>
			</div>

			<div id="contactos">

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL8" /></label>
					<div class="col-md-8 col-contact">
						<s:select id="slcListTipoContactos" name="listTiposContactos"
							headerKey="-1" headerValue="Select Tipo" cssClass="form-control"
							list="#{'1':'Teléfono fijo', '2':'Teléfono celular', '3':'Correo electrónico'}"
							value="selectedTipo" required="true" />
					</div>
				</div>

				<div class="form-group">
					<label class="col-md-4 control-label" for=""><s:text
							name="IU1.4-2_LBL9" /></label>
					<div class="col-md-8 radio-control">
						<div class="form-control-radio pull-left">
							<s:set var="varChecked" value="" />
							<s:iterator value="listCategoriasContacto" var="g">
								<span class="col-md-4 radio-container"> <input
									id="${g[1]}" name="rdCategoria" type="radio" value="${g[1]}"
									onclick="verificaOpcion('rdCategoria_${g[1]}')" />${g[0]}
								</span>
							</s:iterator>
						</div>
					</div>
				</div>
				<s:form id="frmContactoTelefono"
					action="%{#pageContext.request.contextPath}/" theme="simple"
					method="post">
					<div id="divTelefono" class="form-group hidden">
						<s:hidden id="hdnIdContactoTelefono" name="IdcontactoSelTelefono" />
						<s:hidden id="hdnIdTipoContactoTelefono"
							name="IdTipoContactoTelefono" />
						<s:hidden id="hdnIdCategoriaTelefono" name="IdCategoriaTelefono" />
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-2_LBL10" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txTelephoneCodeArea" name="codigoArear"
									type="text" cssClass="form-control" placeholder=""
									maxlength="3" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-2_LBL11" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txTelephone" name="numTelefono" type="text"
									cssClass="form-control" placeholder="" maxlength="8" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 control-label" for=""><s:text
									name="IU1.4-2_LBL12" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txTelephoneExtension"
									name="numExtensionTelefono" type="text" cssClass="form-control"
									placeholder="" maxlength="5" />
							</div>
						</div>
						<div id="divEmergenciaTelefono" class="form-group hidden">
							<div class="form-group">
								<label class="col-md-4 control-label label-obligatorio" for=""><s:text
										name="IU1.4-2_LBL26" /></label>
								<div class="col-md-8 col-contact">
									<s:textfield id="txEmergenciaTelefono" name="numEmergencia"
										type="text" cssClass="form-control" placeholder=""
										maxlength="100" />
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-4 control-label label-obligatorio" for=""><s:text
										name="IU1.4-2_LBL27" /></label>
								<div class="col-md-8 col-contact">
									<s:textfield id="txRelationTelefono" name="relationTelefono"
										type="text" cssClass="form-control" placeholder=""
										maxlength="50" />
								</div>
							</div>
						</div>
					</div>
				</s:form>
				<s:form id="frmContactoMovil"
					action="%{#pageContext.request.contextPath}/" theme="simple"
					method="post">
					<div id="divMovil" class="form-group hidden">
						<s:hidden id="hdnIdContactoMovil" name="contactoSel.id" />
						<s:hidden id="hdnIdTipoContactoMovil" name="contactoSel.idTipo" />
						<s:hidden id="hdnIdCategoriaMovil" name="contactoSel.idCategoria" />
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-2_LBL10" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txMobileCodeArea" name="contactoSel.auxiliar"
									type="text" cssClass="form-control" placeholder=""
									maxlength="3" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-2_LBL28" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txMobileTelephone" name="contactoSel.contacto"
									type="text" cssClass="form-control" placeholder=""
									maxlength="10" />
							</div>
						</div>
						<div id="divEmergenciaMovil" class="form-group hidden">
							<div class="form-group">
								<label class="col-md-4 control-label label-obligatorio" for=""><s:text
										name="IU1.4-2_LBL26" /></label>
								<div class="col-md-8 col-contact">
									<s:textfield id="txEmergenciaMovil"
										name="contactoSel.emergencia" type="text"
										cssClass="form-control" placeholder="" maxlength="100" />
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-4 control-label label-obligatorio" for=""><s:text
										name="IU1.4-2_LBL27" /></label>
								<div class="col-md-8 col-contact">
									<s:textfield id="txRelationMovil" name="contactoSel.relation"
										type="text" cssClass="form-control" placeholder=""
										maxlength="50" />
								</div>
							</div>
						</div>
					</div>
				</s:form>
				<s:form id="frmContactoEmail"
					action="%{#pageContext.request.contextPath}/" theme="simple"
					method="post">
					<div id="divEmail" class="form-group hidden">
						<s:hidden id="hdnIdContactoEmail" name="contactoSel.id" />
						<s:hidden id="hdnIdTipoContactoEmail" name="contactoSel.idTipo" />
						<s:hidden id="hdnIdCategoriaEmail" name="contactoSel.idCategoria" />
						<div class="form-group">
							<label class="col-md-4 control-label label-obligatorio" for=""><s:text
									name="IU1.4-2_LBL29" /></label>
							<div class="col-md-8 col-contact">
								<s:textfield id="txEmail" name="contactoSel.contacto"
									type="text" cssClass="form-control" placeholder=""
									maxlength="50" />
							</div>
						</div>
						<div id="divEmergenciaEmail" class="form-group hidden">
							<div class="form-group">
								<label class="col-md-4 control-label label-obligatorio" for=""><s:text
										name="IU1.4-2_LBL26" /></label>
								<div class="col-md-8 col-contact">
									<s:textfield id="txEmergenciaEmail"
										name="contactoSel.emergencia" type="text"
										cssClass="form-control" placeholder="" maxlength="100" />
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-4 control-label label-obligatorio" for=""><s:text
										name="IU1.4-2_LBL27" /></label>
								<div class="col-md-8 col-contact">
									<s:textfield id="txRelationEmail" name="contactoSel.relation"
										type="text" cssClass="form-control" placeholder=""
										maxlength="50" />
								</div>
							</div>
						</div>
					</div>
				</s:form>
				<!-- Boton que llama al dialog, se llama con medios-contacto.js -->

				<div class="form-group">
					<div class="col-md-12">
						<a id="btnAgregar" class="btn btn-default btn-default-eld"
							onclick="agregar()"><s:text name="IU1.4-2_BTNAGREGAR" /></a>
					</div>
				</div>

				<!-- Lista de contactos -->
				<div class="row">
					<div id="divContactos" class="col-md-12">
						<div class="contactos-container">
							<s:fielderror fieldName="contactoFieldError"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />
							<s:if test="%{#listContactosSel.size > 0}">
								<s:set var="tltEdit" value="%{getText('COM.SPRACH.TLT_EDITAR')}" />
								<s:set var="tltDelete"
									value="%{getText('COM.SPRACH.TLT_DELETE')}" />
								<table id="tblContactos">
									<!-- 			<s:iterator value="listContactosSel" var="c">
										<tr>
											<td>${c[0]},${c[1]},${c[2]}</td>
											<td class="col-buttons"><img class="button-action-image"
												src="${pageContext.request.contextPath}/resources/images/icons/edit.png"
												title="${tltEdit}" alt="${tltEdit}"
												onclick="editarContacto(${c[3]})" /><img
												class="button-action-image"
												src="${pageContext.request.contextPath}/resources/images/icons/delete.png"
												title="${tltDelete}" alt="${tltDelete}"
												onclick="eliminarContacto(${c[3]})" /></td>
										</tr>
									</s:iterator>  -->
								</table>
							</s:if>
							<s:else>
								<div class="contact-text">
									<s:text name="Aún no se han agregado contactos" />
								</div>
							</s:else>
						</div>
					</div>
				</div>
				<!-- PENDIENTE ACABAR MEDIOS DE CONTACTOS -->
			</div>

			<!-- Pestaña Domicilio -->

			<div id="domicilio">

				<!-- Código Postal -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL13" /></label>
					<div class="col-md-8">
						<s:textfield id="txCp" name="model.cp" type="text"
							cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL13')}"
							maxlength="20" cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!-- Entidad federativa -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL14" /></label>
					<div class="col-md-8">
						<s:textfield id="txEntidadFederativa"
							name="model.entidadFederativa" type="text"
							cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL14')}"
							maxlength="20" cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!-- Delegación/municipio -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL15" /></label>
					<div class="col-md-8">
						<s:textfield id="txMunicipio" name="model.municipio" type="text"
							cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL15')}"
							maxlength="20" cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!--Colonia -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL16" /></label>
					<div class="col-md-8">
						<s:textfield id="txColonia" name="model.colonia" type="text"
							cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL16')}"
							maxlength="20" cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!-- Calle -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL17" /></label>
					<div class="col-md-8">
						<s:textfield id="txCalle" name="model.calle" type="text"
							cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL17')}"
							maxlength="20" cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!-- Número exterior -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL18" /></label>
					<div class="col-md-8">
						<s:textfield id="txNumeroExterior" name="model.numeroExterior"
							type="text" cssClass="form-control"
							placeholder="%{getText('IU1.4-2_LBL18')}" maxlength="20"
							cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!-- Número interior -->

				<div class="form-group">
					<label class="col-md-4 control-label" for=""><s:text
							name="IU1.4-2_LBL19" /></label>
					<div class="col-md-8">
						<s:textfield id="txNumeroInterior" name="model.numeroInterior"
							type="text" cssClass="form-control"
							placeholder="%{getText('IU1.4-2_LBL19')}" maxlength="20"
							cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>
			</div>
			<div id="informacionEscolar">

				<!-- Nombre secundaria -->

				<div class="form-group">
					<label class="col-md-4 control-label" for=""><s:text
							name="IU1.4-2_LBL20" /></label>
					<div class="col-md-8">
						<s:textfield id="txNombreSecundaria" name="model.nombreSecundaria"
							type="text" cssClass="form-control"
							placeholder="%{getText('IU1.4-2_LBL20')}"
							cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>

				<!-- Promedio secundaria -->

				<div class="form-group">
					<label class="col-md-4 control-label" for=""><s:text
							name="IU1.4-2_LBL21" /></label>
					<div class="col-md-8">
						<s:textfield id="txPromedioSecundaria"
							name="model.promedioSecundaria" type="text"
							cssClass="form-control" placeholder="%{getText('IU1.4-2_LBL21')}"
							cssErrorClass="field-error" />
						<!-- 	<s:fielderror fieldName="model.nombre" cssClass="error"
								theme="%{getText('mx.edu.eld.defaulTheme')}" />  -->
					</div>
				</div>


				<!-- Certificado Secundaria -->

				<div class="form-group">
					<label class="col-md-4 control-label label-obligatorio" for=""><s:text
							name="IU1.4-2_LBL22" /></label>
					<div class="col-md-8 pull-left">
						<span id="certificadoSecundaria" class="col-md-4" for=""><s:text
								name="LBL_ARCHIVO" /></span>
						<div class="input-group col-md-8">
							<input type="hidden" class="form-control" /> <span
								class="input-group-btn pull-left" id="loadImage"> <i
								class="material-icons md-24 md-eld"
